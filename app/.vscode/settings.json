{
  "python.analysis.typeCheckingMode": "basic",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.ruff": "explicit",
      "source.organizeImports.ruff": "explicit"
    }
  },
  "python-envs.defaultEnvManager": "ms-python.python:conda",
  "python-envs.defaultPackageManager": "ms-python.python:conda",
  "python-envs.pythonProjects": [],
  "geminicodeassist.chat.automaticScrolling": false,
  "geminicodeassist.chat.changeView": "Default diff view",
  "geminicodeassist.chat.defaultCodeBlockDisplay": "Collapsed",
  "geminicodeassist.chat.enableStreaming": false,
  "geminicodeassist.displayInlineContextHint": false,
  "geminicodeassist.enableTelemetry": false,
  "geminicodeassist.inlineSuggestions.enableAuto": true
}
